# DataImpulse Proxy username:password@domain:port
PROXY=username:password@domain:port


# # Email Configuration
# Email that will receive alerts
RECEIVER_EMAIL=receiver_email_address

# Gmail address used to send alerts
SENDER_EMAIL=sender_email_address

# Gmail App Password (generated from Google App Passwords)
SENDER_EMAIL_APP_PASSWORD=APP_PASSWORD

# Turn email alerts ON/OFF (true/false)
EMAIL_ALERTS_ENABLED=true